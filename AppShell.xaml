<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="BOMS.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:BOMS"
    xmlns:pages="clr-namespace:BOMS.Pages"
    Title="StarbucksOrderSystem"
    Shell.TabBarIsVisible="True"
    Shell.NavBarIsVisible="True">

    <TabBar>
        <ShellContent
            Title="Home"
            Route="Home"
            ContentTemplate="{DataTemplate local:MainPage}" />
        <ShellContent
            Title="Telemetry"
            Route="Telemetry"
            ContentTemplate="{DataTemplate pages:TelemetryPage}" />
    </TabBar>

</Shell>
